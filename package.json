{
  "name": "react-quick-start",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "craco build",
    "dev": "craco start",
    "eject": "react-scripts eject",
    "test": "craco test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
